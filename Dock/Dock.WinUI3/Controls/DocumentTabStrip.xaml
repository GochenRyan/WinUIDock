    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dock.WinUI3.Controls" xmlns:controls="using:CommunityToolkit.WinUI.Controls">

    <Style TargetType="local:DocumentTabStrip">
        <!--<Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:DocumentTabStrip">
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>-->
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <controls:WrapPanel Name="HorizontalWrapPanel" Margin="2" />
