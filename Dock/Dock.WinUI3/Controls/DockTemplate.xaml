<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dock.WinUI3.Controls">

    <DataTemplate x:Key="RootDockTemplate">
        <local:RootDockControl ItemsSource="{Binding VisibleDockables, Mode=OneWay}"/>
    </DataTemplate>

    <DataTemplate x:Key="DocumentDockTemplate">
        <local:DocumentDockControl Content="{Binding VisibleDockables, Mode=OneWay}"/>
    </DataTemplate>

    <DataTemplate x:Key="ProportionDockTemplate">
        <local:ProportionalDockControl ItemsSource="{Binding VisibleDockables, Mode=OneWay}"/>
    </DataTemplate>

    <DataTemplate x:Key="SplitterTemplate">
        <local:ProportionalStackPanelSplitter/>
    </DataTemplate>

    <DataTemplate x:Key="ToolDockTemplate">
        <local:ToolDockControl Content="{Binding VisibleDockables, Mode=OneWay}"/>
    </DataTemplate>

</ResourceDictionary>
