<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dock.WinUI3.Controls">
    
    <Style TargetType="local:DocumentDockControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:DocumentDockControl">
                    <local:DockableControl TrackingMode="Visible">
                        <local:DocumentControl />
                    </local:DockableControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--<Setter Property="ContentTemplateSelector">
            <Setter.Value>
                <local:DocumentDockSelector DocumentTemplate="{StaticResource DocumentTemplate}"/>
            </Setter.Value>
        </Setter>-->
    </Style>
    
</ResourceDictionary>
